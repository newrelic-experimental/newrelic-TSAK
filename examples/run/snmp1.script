stdlib = import("stdlib")
fmt = import("fmt")
nsnmp = import("protocols/snmp")
snmp = import("protocols/snmp/client")
log = import("tlog")

nsnmp.InitMib("/usr/share/snmp/mibs")
nsnmp.LoadModule("IF-MIB")
fmt.Println("Got from SNMPv1", snmp.Getv1("10.0.0.162:161", "public", "1.3.6.1.2.1.1.1.0", 1))
fmt.Println("Got from SNMPv2c", snmp.Getv2c("10.0.0.162:161", "public", nsnmp.OID("SNMPv2-MIB::sysORUpTime.1"), 1))
fmt.Println("Got from SNMPv2c", snmp.Getv2c("10.0.0.162:161", "public", nsnmp.OID("1.3.6.1.2.1.1.1.0"), 1))

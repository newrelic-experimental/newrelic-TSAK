stdlib = import("stdlib")
fmt = import("fmt")
net = import("net")
log = import("tlog")
dtls = import("net/dtls")
os = import("os")

fmt.Println("Current directory", os.Getwd() )
listen, err = dtls.New("127.0.0.1", 60001, "./examples/certificates/server.pem", "./examples/certificates/server.pub.pem")
conn, err = listen.Accept()
buf = make([]byte, 3000)
conn.Read(buf) 
fmt.Printf("Data received", stdlib.String(buf))
con.Close()
log.Trace("We are done here")

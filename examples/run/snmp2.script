stdlib = import("stdlib")
fmt = import("fmt")
nsnmp = import("snmp")
snmp = import("snmp/client")
log = import("tlog")

nsnmp.InitMib("/usr/share/snmp/mibs")
nsnmp.LoadModule("IF-MIB")
res = snmp.Walk("10.0.0.162:161", "public", ".1.3.6.1.2.1.1.9.1.4", 1)
for v in res {
  fmt.Printf("%s -> %s \n", v.Oid, v.Variable)
}
